    "express": "^4.16.0",
    "html-to-draftjs": "^1.4.0",
    "humps": "^2.0.1",
    "i18next": "^11.3.1",
    "i18next-browser-languagedetector": "^2.2.0",
    "lodash": "^4.17.19",
    "prop-types": "^15.6.0",
    "query-string": "^6.2.0",
