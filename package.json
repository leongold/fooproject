{
  "name": "olympus_client",
  "version": "1.8.0",
  "private": true,
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "setup": "yarn install && node scripts/setup.js",
    "build": "yarn run clean && webpack --config ./config/webpack.config.production.js",
    "build:development": "webpack-dev-server --config ./config/webpack.config.development.js",
    "clean": "rm -rf build/*",
    "lint": "eslint src/ --max-warnings=0",
    "start": "yarn install && yarn run build:development",
    "server": "node server.js",
    "load-env": "node scripts/load-env.js",
    "test": "yarn install && node scripts/test-suite.js",
    "test:unit": "jest --coverage",
    "test:integration": "cypress run",
    "seed:test": "echo 'No script specified, seed skipped'",
    "report-coverage": "codeclimate-test-reporter < coverage/lcov.info",
    "heroku-prebuild": "yarn remove node-sass",
    "heroku-postbuild": "yarn run build",
    "generate-bundle-stats": "yarn run clean && webpack --config config/webpack.config.production.js --profile --json > .webpack-stats.json",
    "bundle-analyzer-server": "webpack-bundle-analyzer .webpack-stats.json build/",
    "analyze-bundle": "echo 'Building bundle...' && yarn run generate-bundsle-stats && yarn run bundle-analyzer-server"
  },
  "dependencies": {
    "@launchpadlab/lp-components": "^3.16.1",
    "@launchpadlab/lp-form": "^2.2.1",
